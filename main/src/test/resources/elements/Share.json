{
    "shareBtn":
    {
        "SimpleName": "Share Button",
        "iOS":
        {
            "AllApps":
            {
                "AllDevices": "IosNsPredicate::wdType == 'XCUIElementTypeButton' AND wdName == 'Share Button'"
            }
        },
        "Android":
        {
            "AllApps":
            {
                "AllDevices": "AndroidUIAutomator::resourceId(\"$inPackage:id/action_share\")"
            }
        }
    },
    "shareTtl":
    {
        "SimpleName": "Share Title",
        "Android":
