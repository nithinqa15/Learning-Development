{
	"allShowsBtn":
	{
		"SimpleName": "All Shows Button",
		"iOS":
		{
			"AllApps":
			{
				"AllDevices": "IosNsPredicate::wdType == 'XCUIElementTypeButton' AND wdName == 'Shows'"
			}
		},
		"Android":
		{
			"AllApps":
			{
				"AllDevices": "AndroidUIAutomator::resourceId(\"$inPackage:id/action_shows\")"
			}
		}
	},
