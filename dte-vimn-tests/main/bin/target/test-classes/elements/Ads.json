{
    "playAdsBtn":
    {
        "SimpleName": "Play Ads Button",
        "iOS":
        {
            "AllApps":
            {
                "AllDevices": "IosNsPredicate::wdType == 'XCUIElementTypeButton' AND wdName == 'Main Play Button'"
            }
        },
        "Android":
        {
            "AllApps":
            {
                "AllDevices": "AndroidUIAutomator::resourceId(\"$inPackage:id/controls_play_pause\")"
            }
        }
    },
    "pauseAdsBtn":
    {
        "SimpleName": "Pause Ads Button",
